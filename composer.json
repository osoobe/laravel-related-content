{
    "name": "osoobe/laravel-related-content",
    "description": "A description for related-content.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Oshane Bailey",
            "email": "b4.oshany@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0",
        "osoobe/laravel-traits": "*"
    },
    "autoload": {
        "psr-4": {
